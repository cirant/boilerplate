{
  "name": "node-boilerplate",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "set NODE_ENV=development && nodemon --exec babel-node ./bin/www",
    "lint": "eslint --fix --ignore-path .gitignore ./ && echo Lint Passed",
    "start": "node babel-node ./bin/www",
    "test": "set NODE_ENV=test && jest --detectOpenHandles --runInBand --forceExit",
    "coverage": "jest --detectOpenHandles --coverage --forceExit"
  },
  "dependencies": {
    "bcrypt": "^3.0.6",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^8.0.0",
    "express": "~4.17.3",
    "express-jwt": "^5.3.1",
    "http-errors": "~1.6.2",
    "jade": "~1.11.0",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.6.0",
    "morgan": "~1.9.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/node": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "babel-polyfill": "^6.26.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.17.3",
    "expect": "^24.8.0",
    "nodemon": "^1.19.1",
    "supertest": "^4.0.2"
  }
}
